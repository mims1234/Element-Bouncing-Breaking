{
    "manifest_version": 3,
    "name": "Element Bounce and Break - MiMs",
    "version": "1.0",
    "description": "Click on elements to make them bounce and break!",
    "permissions": ["activeTab"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_title": "Activate Element Bounce and Break"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"],
        "css": []
      }
    ]
  }
  